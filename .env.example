# Jen AI Assistant - Environment Configuration Template
# Copy this file to .env and update with your actual credentials

# Database Configuration
SQLSERVER_HOST=your_sql_server_host
SQLSERVER_DB=your_database_name
SQLSERVER_USER=your_username
SQLSERVER_PASSWORD=your_password
SQLSERVER_PORT=1433

# AI Service Configuration
OPENAI_API_KEY=your_openrouter_api_key
OPENAI_BASE_URL=https://openrouter.ai/api/v1
OPENAI_MODEL=openai/gpt-4o-mini

# ElevenLabs Voice Configuration
ELEVENLABS_API_KEY=your_elevenlabs_api_key
JEN_VOICE_ID=your_voice_id
JEN_AGENT_ID=your_agent_id
JEN_PHONE_NUMBER_SID=your_phone_number_sid

# Twilio Configuration (Optional)
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_PHONE_NUMBER=your_twilio_phone_number

# Authentication
JEN_API_KEY=JenAI2025
API_SECRET_KEY=EquityRachel2025ChatAPI

# Webhook Configuration
JEN_WEBHOOK_URL=https://jen-ai.onrender.com/elevenlabs/webhook

# Server Configuration
PORT=8000
HOST=0.0.0.0
ENVIRONMENT=production
LOG_LEVEL=INFO

# Performance Settings
MAX_WORKERS=1
TIMEOUT_SECONDS=30
CACHE_TTL_SECONDS=300

# CORS Settings
ALLOWED_ORIGINS=*
ALLOWED_METHODS=GET,POST,PUT,DELETE,OPTIONS
ALLOWED_HEADERS=*